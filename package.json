    "feathers-socketio": "^2.0.1",
    "feathers-vuex": "^1.0.4",
    "helmet": "^3.9.0",
    "js-cookie": "^2.2.0",
    "moment": "^2.19.2",
    "mongoose": "^4.13.5",
    "nuxt": "^1.0.0-rc3",
    "serve-favicon": "^2.4.5",
    "socket.io-client": "^2.0.4",
    "source-map-support": "^0.5.0",
