{
  "name": "landestool",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "",
  "private": true,
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "backpack dev",
    "build": "nuxt build && backpack build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --config .eslintrc.js .",
    "lint:fix": "eslint server/. server/test/. --config .eslintrc.js --fix",
    "start": "node build/main.js",
    "precommit": "npm run lint",
    "test": "npm run lint && npm run test:server",
    "test:server": "npm run mocha",
    "mocha": "nyc --reporter=html --reporter=text  mocha server/test/ --recursive --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "heroku-postbuild": "npm run build",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.2",
    "@feathersjs/authentication-client": "^1.0.2",
    "@feathersjs/authentication-jwt": "^2.0.0",
    "@feathersjs/authentication-local": "^1.1.0",
    "@feathersjs/client": "^3.4.2",
    "@feathersjs/configuration": "^1.0.2",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/express": "^1.2.0",
    "@feathersjs/feathers": "^3.1.3",
    "@feathersjs/socketio": "^3.2.0",
    "@feathersjs/socketio-client": "^1.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^4.0.0",
    "compression": "^1.7.2",
    "cookie-storage": "^3.1.0",
    "cors": "^2.8.4",
    "cross-env": "^5.1.3",
    "express": "^4.15.3",
    "feathers-authentication-hooks": "^0.2.0",
    "feathers-blob": "^2.0.0",
    "feathers-hooks": "^2.1.2",
    "feathers-hooks-common": "^4.8.0",
    "feathers-mongoose": "^6.0.0",
    "feathers-nedb": "^3.0.0",
    "feathers-vuex": "^1.1.4",
    "fs-blob-store": "^5.2.1",
    "helmet": "^3.11.0",
    "js-cookie": "^2.2.0",
    "moment": "^2.20.1",
    "mongoose": "^5.0.7",
    "nedb": "^1.8.0",
    "nuxt": "^1.4.0",
    "serve-favicon": "^2.4.5",
    "socket.io-client": "^2.0.4",
    "source-map-support": "^0.5.3",
    "vue-avatar": "^2.1.3",
    "vue-bootstrap-datetimepicker": "^4.1.3",
    "vue-datetime-picker": "^0.2.1",
    "vue-form-wizard": "^0.8.4",
    "vue-password": "^1.1.2",
    "vue2-dropzone": "^3.0.6",
    "vuejs-datepicker": "^1.1.0",
    "vuejs-dialog": "^0.8.0",
    "winston": "^3.1.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "directories": {
    "lib": "server",
    "test": "server/test/"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babili-webpack-plugin": "0.1.1",
    "backpack-core": "^0.5.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.2",
