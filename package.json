    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^5.0.1",
    "node-sass": "^4.6.0",
    "nyc": "^11.5.0",
    "sass-loader": "^7.0.1",
    "supertest": "^3.0.0",
    "vue-styleguidist": "^1.4.8"
  }
